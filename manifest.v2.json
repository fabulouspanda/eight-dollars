{
    "manifest_version": 2,
    "name": "Identify the MAGA fans on Twitter",
    "version": "1",
    "description": "This extension shows paid twitter users as MAGA fans, and legacy verified users as verified",
    "icons": {
        "16": "./assets/16.png",
        "32": "./assets/32.png",
        "48": "./assets/48.png",
        "256": "./assets/256.png",
        "512": "./assets/512.png"
    },
    "content_scripts": [
        {
            "matches": [
                "https://twitter.com/*",
                "https://mobile.twitter.com/*"
            ],
            "js": [
                "content.js"
            ]
        }
    ],
    "web_accessible_resources": [
        "script.js",
        "search.js"
    ],
    "permissions": [
        "storage"
    ],
    "browser_action": {
        "default_icon": "./assets/32.png",
        "default_title": "Eight-Dollars",
        "default_popup": "options/options.html"
    }
}